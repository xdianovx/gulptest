{
  "extends": ["airbnb-base", "some-other-config-you-use", "prettier"],
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": true
  },

  "rules": {
    "semi": 0,
    "comma-dangle": ["error", "never"],
    "import/no-extraneous-dependencies": [
      "error",
      { "devDependencies": ["gulpfile.babel.js", "gulp/**/*"] }
    ]
  }
}
